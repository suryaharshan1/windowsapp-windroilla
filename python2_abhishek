list=[67,89,23,25,67,2,7]
 
list.append(8)
print list

print list.pop()
print list

list.reverse()
print list
list.sort()
print list

def trim(ele):
    return ele%3 !=0 and ele%5 !=0

print filter(trim,range(2,100))

def cube(x):
    return x**3

print map(cube,range(1,100))

print sum(range(1,10))
